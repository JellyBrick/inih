all:
	g++ -mwindows -D_LARGEFILE_SOURCE -D_LARGEFILE64_SOURCE -o ini_buffer.c.exe ../ini.c ini_buffer.c -O4 -march=native -fpermissive -funroll-loops -mfpmath=sse -msse2 -msse3 -mssse3 -ffast-math  -Wno-format-y2k  -fno-exceptions -fno-strict-aliasing -s -fopenmp -std=c++0x -mconsole -m64 -static-libgcc -static-libstdc++ -static
	g++ -mwindows -D_LARGEFILE_SOURCE -D_LARGEFILE64_SOURCE -o ini_dump.c.exe ../ini.c ini_dump.c -O4 -march=native -fpermissive -funroll-loops -mfpmath=sse -msse2 -msse3 -mssse3 -ffast-math  -Wno-format-y2k  -fno-exceptions -fno-strict-aliasing -s -fopenmp -std=c++0x -mconsole -m64 -static-libgcc -static-libstdc++ -static
	g++ -mwindows -D_LARGEFILE_SOURCE -D_LARGEFILE64_SOURCE -o ini_example.c.exe ../ini.c ini_example.c -O4 -march=native -fpermissive -funroll-loops -mfpmath=sse -msse2 -msse3 -mssse3 -ffast-math  -Wno-format-y2k  -fno-exceptions -fno-strict-aliasing -s -fopenmp -std=c++0x -mconsole -m64 -static-libgcc -static-libstdc++ -static
	g++ -mwindows -D_LARGEFILE_SOURCE -D_LARGEFILE64_SOURCE -o ini_xmacros.c.exe ../ini.c ini_xmacros.c -O4 -march=native -fpermissive -funroll-loops -mfpmath=sse -msse2 -msse3 -mssse3 -ffast-math  -Wno-format-y2k  -fno-exceptions -fno-strict-aliasing -s -fopenmp -std=c++0x -mconsole -m64 -static-libgcc -static-libstdc++ -static